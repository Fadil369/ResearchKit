      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "ResearchKit is an open source framework and is not shipped with native IOS. To use it, you can embed ResearchKit into your app as dynamic framework.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Installation",
          "level" : 3,
          "text" : "Installation",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Clone the latest stable version of ResearchKit.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "git clone -b stable https:\/\/github.com\/ResearchKit\/ResearchKit.git"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Or, for the latest changes, use the main branch:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "git clone https:\/\/github.com\/ResearchKit\/ResearchKit.git"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "anchor" : "Add-the-ResearchKit-framework-to-your-app",
          "level" : 3,
          "text" : "Add the ResearchKit framework to your app",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "To get started, drag ResearchKit.xcodeproj from your checkout into your iOS app project in Xcode:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "setting-up-your-project-to-use-researchkit-1",
              "type" : "image"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Embed the ResearchKit framework as a dynamic framework in your app by adding it to the Embedded Binaries section of the General pane for your target as shown in the figure below.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "setting-up-your-project-to-use-researchkit-2",
              "type" : "image"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Import-the-correct-modules",
          "level" : 3,
          "text" : "Import the correct modules",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "If you only want to present Consent and Survey UI, import ResearchKit & ResearchKitUI.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "import ResearchKit",
            "import ResearchKitUI"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "If you also want to use ResearchKit Active Tasks, import ResearchKitActiveTask",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "import ResearchKit",
            "import ResearchKitUI",
            "import ResearchKitActiveTask"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
